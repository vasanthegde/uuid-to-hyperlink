{
  "manifest_version": 3,
  "name": "UUID Link Converter",
  "version": "1.0",
  "description": "Converts UUIDs to clickable hyperlinks with staging/prod URL handling",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "UUID Link Converter Settings"
  },
  "icons": {
  }
}
